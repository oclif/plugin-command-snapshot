{
  "name": "@oclif/plugin-command-snapshot",
  "description": "generates and compares OCLIF plugins snapshot files",
  "version": "3.1.2",
  "author": "Salesforce",
  "bugs": "https://github.com/oclif/plugin-command-snapshot/issues",
  "dependencies": {
    "@oclif/core": "^1.0.1",
    "chalk": "^4.1.1",
    "just-diff": "^3.1.1",
    "semver": "^7.3.5",
    "sinon": "^11.1.1",
    "ts-json-schema-generator": "^0.93.0",
    "tslib": "^2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.4",
    "@oclif/plugin-help": "^5.1.10",
    "@oclif/test": "^1.2.9",
    "@types/chai": "^4",
    "@types/mocha": "^8",
    "@types/node": "^15",
    "@types/semver": "^7.3.9",
    "chai": "^4",
    "commitlint": "^12.1.4",
    "eslint": "^7",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2",
    "globby": "^11",
    "husky": "6",
    "mocha": "^8",
    "nyc": "^15",
    "oclif": "^2.0.1",
    "ts-node": "^9",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/schemas",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/oclif/plugin-command-snapshot",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "topics": {
      "snapshot": {
        "description": "generates and compares OCLIF plugins snapshot files"
      },
      "schema": {
        "description": "generates and compares OCLIF plugins schema files"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "oclif/plugin-command-snapshot",
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "commitlint": "commitlint",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif manifest . && oclif readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" --timeout 20000",
    "version": "oclif readme && git add README.md"
  }
}
